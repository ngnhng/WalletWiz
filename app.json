{
    "expo": {
        "name": "WalletWiz",
        "slug": "walletwiz",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "dark",
        "scheme": "wallet-wiz",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.tryz.WalletWiz"
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro"
        },
        "plugins": ["expo-router"],
        "extra": {
            "eas": {
                "projectId": "756e67df-1562-4452-be7f-e9cdf1306bfb"
            }
        },
        "owner": "nguyendhst"
    }
}
